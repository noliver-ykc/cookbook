nst kabochaSoup = {
  Kabocha: 1200,
  Butter: 2, 
  HeavyCream: 200
}

const MacNCheese = {
  MacNCheeseMix: 3,
  Butter: 3
}

function ingredientSum(...objs) {
  return objs.reduce((a, b) => {
    for (let k in b) {
      if (b.hasOwnProperty(k))
        a[k] = (a[k] || 0) + b[k];
    }
    return a;
  }, {});
}

console.log(ingredientSum(kabochaSoup, MacNCheese));
